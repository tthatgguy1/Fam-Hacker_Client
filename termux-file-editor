echo " "
date +"%l:%M:%S %p" | lolcat --animate -d 15

## File Info
cd downloads
newf=$(ls -Art | tail -n 1)
echo " "
cowsay -r "${newf}" | lolcat
echo " "

## Aria2 Download
echo " "
echo -e "To \033[1;31mQuit\033[0m press \033[1;31mctrl\033[0m \033[1;33m+\033[0m \033[1;31mC\033[0m \033[1;33mtogether\033[0m, and then press \033[1;33mEnter\033[0m"
echo " "
mpv ~/storage/shared/Nah-Fam-Torrent-Kit/.NFS-Data/sfx/Cooking_Great.mp3 >/dev/null 2>/dev/null
echo " "
"aria2c" --select-file=1 --seed-time=0 --summary-interval=8000 --max-connection-per-server=16 -d /data/data/com.termux/files/home/storage/shared/Nah-Fam's-Torrent-Kit/Fam-Hacker_Client "/data/data/com.termux/files/home/downloads/${newf}"

## Sound 2
echo " "
mpv ~/storage/shared/Nah-Fam-Torrent-Kit/.NFS-Data/sfx/MMF.mp3 >/dev/null 2>/dev/null
mpv ~/storage/shared/Nah-Fam-Torrent-Kit/.NFS-Data/sfx/TLOZ-WW_Heart_Container.mp3 >/dev/null 2>/dev/null
sleep 1.0

## Termux Move
echo " "
echo -e "\e[34mDirecting\e[0m \e[33mTermux\e[0m \e[34mto\e[0m \033[1;36mFam\033[0m\033[1;35m-\033[0m\033[1;36mHacker\033[0m\033[1;35m_\033[0m\033[1;36mClient\033[0m"
