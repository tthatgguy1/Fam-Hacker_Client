## Sound 1
echo "\e[32m "
mpv ~/storage/shared/Nah-Fam-Torrent-Kit/.NFS-Data/sfx/Secret_Sound.mp3
echo "\e[0m "
clear
sleep 2.0

## Torrent Logo & Date
echo '
